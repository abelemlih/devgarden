{
  "master_picture": "app/assets/images/devgarden-favicon.png",
  "favicon_design": {
    "ios": {
      "picture_aspect": "no_change"
    },
    "desktop_browser": [
    ],
    "windows": {
      "picture_aspect": "no_change",
      "background_color": "#282561",
      "on_conflict": "override"
    },
    "android_chrome": {
      "picture_aspect": "no_change",
      "theme_color": "#282561",
      "manifest": {
        "name": "Dev Garden",
        "display": "browser",
        "orientation": "not_set",
        "on_conflict": "override",
        "declared": true
      }
    },
    "safari_pinned_tab": {
      "picture_aspect": "black_and_white",
      "threshold": 50,
      "theme_color": "#282561"
    }
  },
  "settings": {
    "scaling_algorithm": "Mitchell",
    "error_on_image_too_small": true
  }
}
